<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.seesun.mapper.file.FileTypeMapper">
	<!-- 파일 업로드 타입 조회 -->
	<select id="getFileTypeByCode" resultType="fileType">
		SELECT * FROM file_type WHERE code = #{code}
	</select>
</mapper>